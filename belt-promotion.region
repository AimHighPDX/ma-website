                              <li class="past">{startDate|date %m/%d}</li>
                           {.end}
                        {.end}
                     {.end}
                  </ul>
                  {.repeated section upcoming}
                     {.repeated section categories}
                        {.equal? @ "MMA"}
                           <li>{startDate|date %m/%d}</li>
                        {.end}
                     {.end}
                  {.end}
               </ul>
               <div>
                  <h4>Application</h4>
                  <div class="button-set">
                     <button>Beginner</button>
                     <button>Intermediate & Advanced</button>
                  </div>
               </div>
            </section>
   
            <section>
               <h3>Taekwondo</h3>
               <h4>Testing Dates</h4>
               <ul>
                  <ul data-js="reverse-list"> {# This ul will be removed with js}
                     {.repeated section past}
                        {.repeated section categories}
                           {.equal? @ "Taekwondo"}
                              <li class="past">{startDate|date %m/%d}</li>
                           {.end}
                        {.end}
                     {.end}
                  </ul>
                  {.repeated section upcoming}
                     {.repeated section categories}
                        {.equal? @ "Taekwondo"}
                           <li>{startDate|date %m/%d}</li>
                        {.end}
                     {.end}
                  {.end}
               </ul>
               <div>
                  <h4>Application</h4>
                  <div class="button-set">
                     <button>Beginner</button>
                     <button>Intermediate & Advanced</button>
                  </div>
               </div>
            </section>
   
            <section>
               <h3>Tang Soo Do</h3>
               <h4>Testing Dates</h4>
               <ul>
                  <ul data-js="reverse-list"> {# This ul will be removed with js}
                     {.repeated section past}
                        {.repeated section categories}
                           {.equal?:@:"Tang Soo Do"}
                              <li class="past">{startDate|date %m/%d}</li>
                           {.end}
                        {.end}
                     {.end}
                  </ul>
                  {.repeated section upcoming}
                     {.repeated section categories}
                        {.equal?:@:"Tang Soo Do"}
                           <li>{startDate|date %m/%d}</li>
                        {.end}
                     {.end}
                  {.end}
               </ul>
               <div>
                  <h4>Application</h4>
                  <div class="button-set">
                     <button>Beginner</button>
                     <button>Intermediate & Advanced</button>
                  </div>
               </div>
            </section>
         </section>
      </section>
   </main>

   {@|apply footer.block}
</body>

</html>