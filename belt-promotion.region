<!DOCTYPE html>
<html>

<head>
   {squarespace-headers}
   <squarespace:script src="aria.js" />
</head>

<body>
   <squarespace:navigation navigationId="maNav" template="header" />
   <main>
      <section id="page" role="main" data-content-field="main-content">
         <squarespace:block-field id="belt-promotion-intro" label="Belt Promotion Intro" columns="1"/>

         <section>
            <h2>Warriors</h2>
            <h3>Testing Dates</h3>
            <ul>
               {.repeated section past}
                  {.repeated section categories}
                     {.equal? @ "Warriors"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
               {.repeated section upcoming}
                  {.repeated section categories}
                     {.equal? @ "Warriors"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
            </ul>
            <h3>Application</h3>
            <div class="button-set">
               <button>Beginner</button>
               <button>Intermediate & Advanced</button>
            </div>
         </section>

         <section>
            <h2>MMA</h2>
            <h3>Testing Dates</h3>
            <ul>
               {.repeated section past}
                  {.repeated section categories}
                     {.equal? @ "MMA"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
               {.repeated section upcoming}
                  {.repeated section categories}
                     {.equal? @ "MMA"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
            </ul>
            <h3>Application</h3>
            <div class="button-set">
               <button>Beginner</button>
               <button>Intermediate & Advanced</button>
            </div>
         </section>

         <section>
            <h2>Taekwondo</h2>
            <h3>Testing Dates</h3>
            <ul>
               {.repeated section past}
                  {.repeated section categories}
                     {.equal? @ "Taekwondo"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
               {.repeated section upcoming}
                  {.repeated section categories}
                     {.equal? @ "Taekwondo"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
            </ul>
            <h3>Application</h3>
            <div class="button-set">
               <button>Beginner</button>
               <button>Intermediate & Advanced</button>
            </div>
         </section>

         <section>
            <h2>Tang Soo Do</h2>
            <h3>Testing Dates</h3>
            <ul>
               {.repeated section past}
                  {.repeated section categories}
                     {.equal?:@:"Tang Soo Do"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
               {.repeated section upcoming}
                  {.repeated section categories}
                     {.equal?:@:"Tang Soo Do"}
                        <li>{startDate|date %m/%d}</li>
                     {.end}
                  {.end}
               {.end}
            </ul>
            <h3>Application</h3>
            <div class="button-set">
               <button>Beginner</button>
               <button>Intermediate & Advanced</button>
            </div>
         </section>
      </section>
   </main>

   {@|apply footer.block}
</body>

</html>