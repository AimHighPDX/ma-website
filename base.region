<!DOCTYPE html>
<html>

<head>
   {squarespace-headers}
</head>

<body>
   {@|apply header.block}
   <main>
      <!-- Main image Banner -->
      <section>
         <img src="" alt=""> <!-- Refer to page image -->
         <div>
            <squarespace:block-field id="hero-text" columns="1"/>
         </div>
      </section>

      <!-- Summer camps announcement -->
      {@|apply announcement.block}

      <!-- MA Summary -->
      <section class="full-section">
         <div>
            <div>
               <h2>Section 1</h2>
               <squarespace:block-field id="ma-summary-left" columns="1"/>
            </div>
            <div>
               <h2>Section 2</h2>
               <squarespace:block-field id="ma-summary-right" columns="1"/>
            </div>
         </div>
      </section>

      <!-- Program Tabs -->
      {@|apply tabs.block}

      <!-- Events callout -->
      <section class="full-section">
         <div>
            <div>
               <h2>Section 1</h2>
               <squarespace:block-field id="events-summary-left" columns="1"/>
            </div>
            <div>
               <h2>Section 2</h2>
               <squarespace:block-field id="events-summary-right" columns="1"/>
            </div>
         </div>
      </section>

      <!-- Aim High PDX -->
      <section>
         <squarespace:block-field id="ahpdx-about" columns="2"/>
         <button class="secondary">Testing</button>
      </section>
      <!-- Hiring -->
      <section>
         <squarespace:block-field id="ahpdx-hiring" columns="12"/>
      </section>
   </main>

   {@|apply footer.block}
</body>

</html>