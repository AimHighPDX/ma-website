<!DOCTYPE html>
<html>

<head>
   {squarespace-headers}
</head>

<body>
   {@|apply header.block}
   <main data-content-field="main-content">
      <!-- Main image Banner -->
      <section>
         <img src="" alt=""> <!-- Refer to page image -->
         <div>
            <squarespace:block-field id="hero-text" columns="1"/>
         </div>
      </section>

      <!-- Summer camps announcement -->
      {@|apply announcement.block}

      <!-- MA Summary -->
      <section class="full-section">
         <div>
            <h2>Foo</h2>
            <squarespace:block-field id="ma-summary" label="MA Summary" columns="1"/>
         </div>
      </section>

      <!-- Program Tabs 
      {@|apply tabs.block}-->

      <!-- Events callout -->
      <section class="full-section">
         <div>
            <h2>Something</h2>
            <squarespace:block-field id="events-summary" label="Events Summary" columns="1"/>
         </div>
      </section>

      <!-- Aim High PDX -->
      <section>
         <squarespace:block-field id="ahpdx-about" columns="1"/>
         <button class="secondary">Testing</button>
      </section>
      <!-- Hiring -->
      <section>
         <squarespace:block-field id="ahpdx-hiring" columns="1"/>
      </section>
   </main>

   {@|apply footer.block}
</body>

</html>