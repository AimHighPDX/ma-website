<section>
   {.repeated section items}
      <section class="staff">
         <img src="{@|staff-image}" alt="{@|staff-name}">
         <div>
            <h3>{@|staff-name}</h3>
            <h4>{@|staff-role}</h4>
            <p>{@|staff-bio}</p>
         </div>
      </section>
   {.end}
</section>
